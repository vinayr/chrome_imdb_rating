{
  "name": "Show IMDB ratings",
  "description": "Show IMDB ratings",
  "version": "1.4",
  "manifest_version": 2,
  "browser_action": {
    "name": "Show IMDB ratings",
    "icons": ["icon48.png"],
    "default_icon": "icon48.png"
  },
  "content_scripts": [{
    "js": ["jquery-2.0.3.min.js", "showrate.js"],
    "matches": ["https://www.imdb.com/*"]
  }],
  "permissions": [
    "https://www.imdb.com/"
  ],
  "web_accessible_resources": ["jquery-2.0.3.min.map"]
}
